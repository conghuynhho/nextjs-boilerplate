{
  "name": "next13.page.boilerplate",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16.20.0"
  },
  "scripts": {
    "prestart": "npx rimraf .next",
    "prestart:default": "pnpm install",
    "start": "node run-script-os",
    "start:default": "$NVM_DIR/versions/node/v$npm_package_engines_node/bin/node .",
    "start:windows": "%NVM_HOME%/v%npm_package_engines_node%/node .",
    "build:lang": "node run-script-os",
    "build:lang:default": "$NVM_DIR/versions/node/v$npm_package_engines_node/bin/node ./modules/i18n",
    "build:lang:windows": "%NVM_HOME%/v%npm_package_engines_node%/node ./modules/i18n",
    "aws": "node .",
    "preaws:build": "npx rimraf .next && node ./modules/i18n && node ./modules/build-menu-data",
    "aws:build": "next build",
    "lint:js": "eslint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
